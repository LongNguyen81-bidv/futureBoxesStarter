{
  "expo": {
    "name": "FutureBoxes",
    "slug": "futureboxes",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.futureboxes.app"
    },
    "android": {
      "package": "com.futureboxes.app",
      "permissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "plugins": [
      "expo-notifications",
      "expo-font",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "48ed496d-d061-48c7-9b23-34b299def9af"
      }
    }
  }
}
